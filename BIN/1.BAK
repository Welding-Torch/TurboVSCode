#include<iostream.h>
#include<conio.h>

void swapEvenOdd(int foo[], int bar){
 int pos=1; while(pos<=bar){
  if(!pos%2 && pos != bar){
   int tmp = foo[pos-1];
   foo[pos-1] = foo[pos];
   foo[pos] = tmp;
  } pos++;
 }
}

